unit posdesignDelphi.view.componets.navbar;

interface

uses
  System.SysUtils, System.Types, System.UITypes, System.Classes,
  System.Variants,
  FMX.Types, FMX.Controls, FMX.Forms, FMX.Graphics, FMX.Dialogs, FMX.Objects,
  FMX.Layouts, FMX.Controls.Presentation, FMX.StdCtrls,
  posdesignDelphi.view.source.utils;

type
  TComponetsNavBar = class(TForm)
    LayoutContainer: TLayout;
    Rectangle1: TRectangle;
    Layout1: TLayout;
    Image1: TImage;
    SpeedButton1: TSpeedButton;
    procedure FormCreate(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
    function build: TFMXObject;
  end;

var
  ComponetsNavBar: TComponetsNavBar;

implementation

{$R *.fmx}
{ TComponetsNavBar }

function TComponetsNavBar.build: TFMXObject;
begin
  Result := LayoutContainer;
end;

procedure TComponetsNavBar.FormCreate(Sender: TObject);
begin
  TUtils.ResourceImage('img_menu', Image1);
end;

end.
